apiVersion: v1
kind: Secret
metadata:
  name: sparkcreds
type: Opaque
stringData:
  storage-creds.ini: |
    [INPUT]
    AZ_STORAGE_ACCOUNT=
    AZ_CONTAINER=
    AZ_CLIENT_ID=
    AZ_CLIENT_SECRET=
    AZ_CLIENT_ENDPOINT=

    [OUTPUT]
    AZ_STORAGE_ACCOUNT=
    AZ_CONTAINER=
    AZ_CLIENT_ID=
    AZ_CLIENT_SECRET=
    AZ_CLIENT_ENDPOINT=
